<%- include('../includes/head.ejs') %>
    <link rel="stylesheet" href="/css/table.css">
</head>

<body>
    <%- include('../includes/navigation.ejs') %>

    <main>
        <div>
            <h1>VPC Details</h1>
        </div>
        <% if (dataInfo.length > 0) { %>
            <table>
                <thead>
                    <tr>
                        <th>VPC Id</th>
                        <th>Owner Id</th>
                        <th>State</th>
                    </tr>
                </thead>
                <tbody>
                    <% for (let data of dataInfo) { %>
                        <tr>
                           <td>
                            <a href="/<%= region  %>/<%= data.VpcId %>/subnets" class="card-link"><%= data.VpcId %></a>
                            </td>
                           <td><%= data.OwnerId %></td>
                           <td><%= data.State %></td>
                        </tr>
                    <% } %>
                </tbody>               
            </table>
            <% } else { %>
                <h1>No Records Found!</h1>
            <% } %>           
    </main>
<%- include('../includes/end.ejs') %>